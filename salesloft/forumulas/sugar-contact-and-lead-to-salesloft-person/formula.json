{
    "active": true,
    "configuration": [
        {
            "key": "destination",
            "name": "destination",
            "required": false,
            "type": "elementInstance"
        },
        {
            "key": "origin",
            "name": "origin",
            "required": false,
            "type": "elementInstance"
        }
    ],
    "debugLoggingEnabled": false,
    "debugLoggingExpires": "2020-07-21T22:08:30Z",
    "engine": "v3",
    "id": 37550,
    "name": "sugar-contact-and-lead-to-salesloft-person",
    "singleThreaded": false,
    "steps": [
        {
            "name": "processEvent",
            "onFailure": [
                "getSugarLead"
            ],
            "onSuccess": [
                "getSugarContact"
            ],
            "properties": {},
            "type": "filter"
        },
        {
            "name": "getSugarContact",
            "onFailure": [
                "exitSugarContactNotFound"
