            "onFailure": [
                "exitInvalidSugarLead"
            ],
            "onSuccess": [
                "prepCheckPersonFromLeadQuery"
            ],
            "properties": {},
            "type": "filter"
        },
        {
            "name": "prepCheckPersonFromLeadQuery",
            "onFailure": [],
            "onSuccess": [
                "checkIfPersonExistsByLeadEmail"
