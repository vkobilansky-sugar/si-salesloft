            "properties": {
                "api": "/sl-person",
                "elementInstanceId": "${config.destination}",
                "method": "GET",
                "query": "${steps.prepCheckPersonFromLeadQuery.query}"
            },
            "type": "elementRequest"
        },
        {
            "name": "createPayloadForPersonFromLead",
            "onFailure": [],
            "onSuccess": [
                "filterCheckPersonByLeadEmail"
